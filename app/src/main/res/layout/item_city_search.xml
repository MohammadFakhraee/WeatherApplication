<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_gravity="center">

    <TextView
        android:id="@+id/searchedTv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/margin_large"
        android:textSize="@dimen/text_size_h3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="teh" />

    <TextView
        android:id="@+id/completeTv"
        style="@style/DefTxtStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/text_size_h3"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/searchedTv"
        app:layout_constraintStart_toEndOf="@+id/searchedTv"
        app:layout_constraintTop_toTopOf="@+id/searchedTv"
        tools:text="ran" />

    <View
        android:layout_width="0dp"
        android:layout_height="@dimen/line_width"
        android:background="@color/gray200"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>